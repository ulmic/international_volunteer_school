%header.navbar.navbar-default
  .navbar-header
    %button.navbar-toggle{ type: :button, data: { toggle: :collapse, target: '.navbar-collapse' } }
      %span.icon-bar
      %span.icon-bar
      %span.icon-bar
    %a.navbar-brand{ href: root_path }
      = t('application.name')
  .navbar-collapse.collapse
    %ul.nav.navbar-nav
      = menu_item admin_organizers_path do
        = Organizer.model_name.human.pluralize(:ru)
      = menu_item admin_pages_path do
        = Page.model_name.human.pluralize(:ru)
    %ul.nav.navbar-nav.navbar-right
